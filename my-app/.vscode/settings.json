{
  "importSorter.sortConfiguration.customOrderingRules.defaultNumberOfEmptyLinesAfterGroup": 0,
  "importSorter.importStringConfiguration.trailingComma": "multiLine",
  "importSorter.sortConfiguration.customOrderingRules.rules": [
    {
      "regex": "^[^.]",
      "orderLevel": 5
    },
    {
      "regex": "^[.]",
      "orderLevel": 10
    }
  ],
  "importSorter.importStringConfiguration.maximumNumberOfImportExpressionsPerLine.count": 80,
  "importSorter.importStringConfiguration.tabType": "space",
  "importSorter.importStringConfiguration.tabSize": 2,
  "importSorter.generalConfiguration.sortOnBeforeSave": true,
  "importSorter.importStringConfiguration.maximumNumberOfImportExpressionsPerLine.type": "newLineEachExpressionAfterCountLimitExceptIfOnlyOne",
  "workbench.colorCustomizations": {
    "titleBar.activeForeground": "#000",
    "titleBar.inactiveForeground": "#000000CC",
    "titleBar.activeBackground": "#f5d6f9",
    "titleBar.inactiveBackground": "#bba0be"
  },
  // "editor.formatOnSave": false,
  // "[javascript]": {
  //   "editor.formatOnSave": false
  // },
  "prettier.arrowParens": "always",
  "prettier-eslint.eslintIntegration": true,
  "prettier.printWidth": 120,
  "prettier-stylelint.stylelintIntegration": true,
  "flow.pathToFlow": "${workspaceFolder}/node_modules/.bin/flow",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "eslint.autoFixOnSave": true,
  "eslint.alwaysShowStatus": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.fontWeight": "normal"
}
